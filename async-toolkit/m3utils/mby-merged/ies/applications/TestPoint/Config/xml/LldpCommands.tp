# set port config lldp <port> port-id <port-id>
# set port config lldp <port> port-desc <port-description>
# set port config lldp <port> tx-enable port-desc|sys-name|sys-desc|sys-cap|all on|off
# set port config lldp <port> admin tx_only|rx_only|tx_and_rx|disabled

 2,  "port",             "c",    "m",    "",                               "Set port specific properties"
 3,  "config",           "c",    "m",    "",                               "Set port specific configuration"

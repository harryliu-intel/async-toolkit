reg fwd_ieee_reserved_mac_trap_priority_r {
  Security_PolicyGroup     = <%=$Security_Props{'MBY_PG1'}%>;
  Security_ReadAccess_Str  = <%=$Security_Props{'MBY_PG1_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'MBY_PG1_WAC_AGENTS'}%>;
  shared;
  name = "Reserved multicast MAC addresses trap priority select";
  desc = "Configures the default priority behavior for IEEE reserved traps.";
  regwidth = 64;
  accesswidth = 64;
  field {
    AccessType = "RW";
    desc = "
            One bit per MAC address indicating the TC behavior for the 
            frame sent to the CPU when trapped:
          
              0 : TC remains unchanged

              1 : TC changed to the value specified in 
                  IEEE_RESERVED_MAC_CFG
           ";
    ValRandomize = true;
  } SELECT[63:0] = 64'h00;
};

reg fwd_ieee_reserved_mac_cfg_r {
  Security_PolicyGroup     = <%=$Security_Props{'MBY_PG1'}%>;
  Security_ReadAccess_Str  = <%=$Security_Props{'MBY_PG1_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'MBY_PG1_WAC_AGENTS'}%>;
  shared;
  name = "Reserved multicast MAC addresses configuration";
  desc = "Configures the associated TC when the forwarding action for a 
          given IEEE reserved MAC is trap and FWD_IEEE_RESERVED_MAC_TRAP_PRIORITY
          indicates that the TC should be overridden.";
  regwidth = 64;
  accesswidth = 64;
  field {
    AccessType = "RW";
    desc = "
            The TC of a frame with a reserved multicast MAC 
            destination address that is trapped to the CPU when selected 
            with IEEE_RESERVED_MAC_TRAP_PRIORITY
           ";
    ValRandomize = true;
  } TRAP_TC[2:0] = 3'h00;
};

reg fwd_ip_r {
  Security_PolicyGroup     = <%=$Security_Props{'MBY_PG1'}%>;
  Security_ReadAccess_Str  = <%=$Security_Props{'MBY_PG1_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'MBY_PG1_WAC_AGENTS'}%>;
  shared;
  name = "Forward IP registers";
  desc = "Pending bits for generated interrupts.";
  regwidth = 64;
  accesswidth = 64;

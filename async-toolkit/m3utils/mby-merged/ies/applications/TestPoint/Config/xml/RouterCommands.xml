<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE config PUBLIC "-//Fulcrum Microsystems//DTD DefaultCommands/EN" "DefaultCommands.dtd">
<!--
    vim:autoindent:et:sw=4:ts=4:tw=79:
-->

<config>

    <!-- add -->
    <command>
        <name>add</name>

        <!-- add arp-entry -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>arp-entry</name>
            <help>Add a new static ARP entry</help>
            <handler>tpHandleAddArpEntry</handler>

            <argument>
                <name>!&lt;ip address&gt;</name>
                <help>The CIDR style IP address</help>
                <argument>
                    <name>!&lt;DMAC&gt;</name>
                    <help>The destination MAC address</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add a new static ARP entry</help>
                        </argument>
                    </argument>
                </argument>
                <argument>
                    <name>stateless</name>
                    <help>Use the IPv6 stateless autoconfiguration as MAC</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add a new static ARP entry</help>
                        </argument>
                    </argument>
                </argument>
            </argument>
        </command> <!-- add arp-entry -->

        <!-- add interface -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>interface</name>
            <help>Add a new router interface</help>
            <handler>tpHandleAddInterface</handler>

            <argument>
                <name>!&lt;ip address&gt;</name>
                <help>The interface identifier</help>
                <argument>
                    <name>!&lt;vid&gt;</name>
                    <help>The VLAN associated with the interface</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Add a new router interface</help>
                    </argument>
                </argument>
            </argument>
        </command> <!-- add interface -->

        <!-- add route -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>route</name>
            <help>Add a new route</help>
            <handler>tpHandleAddRoute</handler>

            <argument>
                <name>!&lt;subnet&gt;</name>
                <help>The routing target</help>
                <argument>
                    <name>!&lt;gateway&gt;</name>
                    <help>The gateway</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add a new route</help>
                        </argument>
                        <argument>
                            <name>!&lt;vrid&gt;</name>
                            <help>Virtual router ID in the range 1-255</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Add a new route</help>
                            </argument>
                        </argument>
                    </argument>
                </argument>
                <argument>
                    <name>ECMP</name>
                    <help>Use the ECMP Group Id</help>
                    <argument>
                        <name>!&lt;groupId&gt;</name>
                        <help>ECMP Group Id</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add a new route</help>
                        </argument>
                        <argument>
                            <name>!&lt;vrid&gt;</name>
                            <help>Virtual router ID in the range 1-255</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Add a new route</help>
                            </argument>
                        </argument>
                    </argument>
                </argument>
            </argument>
        </command> <!-- add route -->

        <!-- add next-hop -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>next-hop</name>
            <help>Add next hop to an ECMP Group</help>
            <handler>tpHandleAddEcmpGroupNexthop</handler>
            <argument>
                <name>!&lt;groupId&gt;</name>
                <help>ECMP Group Id</help>
                <argument>
                    <name>!&lt;ip address&gt;</name>
                    <help>Next hop Address</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <filter type="family">
                                <deny>FM2000</deny>
                                <deny>FM4000</deny>
                            </filter>
                            <name>!&lt;trapCode&gt;</name>
                            <help>Set the trapCode if the nexthop entry is unresolved</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Add this next hop to the ECMP Group</help>
                            </argument>
                        </argument>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add this next hop to the ECMP Group</help>
                        </argument>
                    </argument>
                </argument>
                
<!-- Eliminate 'raw' mode for now 
                <argument>
                    <filter type="family">
                        <deny>FM2000</deny>
                        <deny>FM4000</deny>
                    </filter>
                    <name>raw</name>
                    <help>Raw next hop address</help>
                    <argument>
                        <name>narrow</name>
                        <help>Add a raw narrow next hop</help>
                        <argument>
                            <name>!&lt;rawData&gt;</name>
                            <help>Raw 64 bit value to set (e.g. 0x0123456789abcdef)</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Add this raw narrow next hop entry to the ECMP Group</help>
                            </argument>
                        </argument>
                    </argument>
                    <argument>
                        <name>wide</name>
                        <help>Add a raw wide next hop</help>
                        <argument>
                            <name>!&lt;first&gt;</name>
                            <help>First raw 64 bit value to set (e.g. 0x0123456789abcdef)</help>
                            <argument>
                                <name>!&lt;second&gt;</name>
                                <help>Second raw 64 bit value to set e.g. 0x0123456789abcdef</help>
                                <argument>
                                    <name>!&lt;cr&gt;</name>
                                    <help>Add this wide next hop entry to the ECMP Group</help>
                                </argument>
                            </argument>
                        </argument>
                    </argument>
                </argument>
(end 'raw' mode) -->
            </argument>
        </command> <!-- add next-hop -->
    </command> <!-- add -->


    <!-- create -->
    <command>
        <name>create</name>

        <!-- create virtual-router -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>virtual-router</name>
            <help>Create a virtual router</help>
            <handler>tpHandleCreateVirtualRouter</handler>

            <argument>
                <name>!&lt;vrid&gt;</name>
                <help>Virtual router ID in the range 1-255</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Create a virtual router</help>
                </argument>
            </argument>
        </command> <!-- create virtual-router -->

        <!-- create ecmp-group -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>ecmp-group</name>
            <help>Create an ECMP Group</help>
            <handler>tpHandleCreateEcmpGroup</handler>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Create an ECMP Group</help>
            </argument>
            <argument>
                <filter type="family">
                    <deny>FM2000</deny>
                    <deny>FM4000</deny>
                </filter>
                <name>wideNextHops</name>
                <help>Create an ECMP Group that make uses of a Wide Next Hop</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Create an ECMP Group that make uses of a Wide Next Hop</help>
                </argument>
            </argument>
        </command> <!-- create ecmp-group -->

    </command> <!-- create -->


    <!-- del -->
    <command>
        <name>del</name>

        <!-- del arp-entry -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>arp-entry</name>
            <help>Delete an ARP entry</help>
            <handler>tpHandleDelArpEntry</handler>

            <!-- del arp-entry <address> -->
            <argument>
                <name>!&lt;ip address&gt;</name>
                <help>The CIDR style IP address</help>

                <!-- del arp-entry <address> interface -->
                <argument>
                    <name>interface</name>
                    <help>Delete ARP entry for specified interface</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete an ARP entry</help>
                        </argument>
                    </argument>
                </argument> <!-- interface -->

                <!-- del arp-entry <address> vlan -->
                <argument>
                    <name>vlan</name>
                    <help>Delete ARP entry for specified vlan</help>
                    <argument>
                        <name>!&lt;vlan&gt;</name>
                        <help>The vlan identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete an ARP entry</help>
                        </argument>
                    </argument>
                </argument> <!-- vlan -->

            </argument> <!-- <address> -->

            <!-- del arp-entry all -->
            <argument>
                <name>all</name>
                <help>Delete all ARP entries</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete all ARP entries</help>
                </argument>
            </argument> <!-- all -->

        </command> <!-- del arp-entry -->

        <!-- del interface -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>interface</name>
            <help>Delete a router interface</help>
            <handler>tpHandleDelInterface</handler>

            <argument>
                <name>!&lt;ip address&gt;</name>
                <help>The interface identifier</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete a router interface</help>
                </argument>
            </argument>
            <argument>
                <name>all</name>
                <help>Delete all router interfaces</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete all router interfaces</help>
                </argument>
            </argument>
        </command> <!-- del interface -->

        <!-- del route -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>route</name>
            <help>Delete a unicast route</help>
            <handler>tpHandleDelRoute</handler>

            <argument>
                <name>!&lt;subnet&gt;</name>
                <help>The routing target</help>
                <argument>
                    <name>!&lt;gateway&gt;</name>
                    <help>The gateway</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete a route</help>
                        </argument>
                        <argument>
                            <name>!&lt;vrid&gt;</name>
                            <help>Virtual router ID in the range 1-255</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Delete a unicast route</help>
                            </argument>
                        </argument>
                    </argument>
                </argument>
                <argument>
                    <name>ECMP</name>
                    <help>Use the ECMP Group Id</help>
                    <argument>
                        <name>!&lt;groupId&gt;</name>
                        <help>ECMP Group Id</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete a unicast route</help>
                        </argument>
                        <argument>
                            <name>!&lt;vrid&gt;</name>
                            <help>Virtual router ID in the range 1-255</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Delete a unicast route</help>
                            </argument>
                        </argument>
                    </argument>
                </argument>
            </argument>
            <argument>
                <name>all</name>
                <help>Delete all unicast routes</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete all unicast routes</help>
                </argument>
            </argument>
        </command> <!-- del route -->

        <!-- del virtual-router -->
        <command>
            <name>virtual-router</name>
            <help>Delete a virtual router</help>
            <handler>tpHandleDelVirtualRouter</handler>

            <argument>
                <name>!&lt;vrid&gt;</name>
                <help>Virtual router ID in the range 1-255</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete a virtual router</help>
                </argument>
            </argument>
            <argument>
                <name>all</name>
                <help>Delete all virtual routers</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete all virtual routers</help>
                </argument>
            </argument>
        </command> <!-- del virtual-router -->

        <!-- del ecmp-group -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>ecmp-group</name>
            <help>Delete an ECMP Group</help>
            <handler>tpHandleDeleteEcmpGroup</handler>
            <argument>
                <name>!&lt;groupId&gt;</name>
                <help>ECMP Group ID to delete</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete an ECMP Group</help>
                </argument>
            </argument>
        </command> <!-- del ecmp-group -->

        <!-- del next-hop -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>next-hop</name>
            <help>Delete next hop from an ECMP Group</help>
            <handler>tpHandleDelEcmpGroupNexthop</handler>
            <argument>
                <name>!&lt;groupId&gt;</name>
                <help>ECMP Group Id</help>
                <argument>
                    <name>!&lt;ip address&gt;</name>
                    <help>Next hop Address</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete this next hop from the ECMP Group</help>
                        </argument>
                    </argument>
                </argument>
<!-- Eliminate 'raw' mode for now 
                <argument>
                    <filter type="family">
                        <deny>FM2000</deny>
                        <deny>FM4000</deny>
                    </filter>
                    <name>raw</name>
                    <help>Raw next hop address</help>
                    <argument>
                        <name>narrow</name>
                        <help>Delete a raw narrow next hop</help>
                        <argument>
                            <name>!&lt;rawData&gt;</name>
                            <help>Raw 64 bit value to delete (e.g. 0x0123456789abcdef)</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Delete this raw narrow next hop entry from the ECMP Group</help>
                            </argument>
                        </argument>
                    </argument>
                    <argument>
                        <name>wide</name>
                        <help>Delete a raw wide next hop</help>
                        <argument>
                            <name>!&lt;first&gt;</name>
                            <help>First raw 64 bit value to delete (e.g. 0x0123456789abcdef)</help>
                            <argument>
                                <name>!&lt;second&gt;</name>
                                <help>Second raw 64 bit value to delete (e.g. 0x0123456789abcdef)</help>
                                <argument>
                                    <name>!&lt;cr&gt;</name>
                                    <help>Delete this wide next hop entry from the ECMP Group</help>
                                </argument>
                            </argument>
                        </argument>
                    </argument>
                </argument>
(end eliminate 'raw' mode) -->
            </argument>
        </command> <!-- del next-hop -->

    </command> <!-- del -->


    <!-- reset -->
    <command>
        <name>reset</name>
        <help>Reset switch, port, vlan, mac, lag properties</help>

        <!-- reset arp-entry -->
        <command>
            <name>arp-entry</name>
            <help>Reset ARP table entry properties</help>

            <!-- reset arp-entry used -->
            <command>
                <name>used</name>
                <help>Reset the "used" attribute of an ARP entry</help>
                <handler>tpHandleResetArpEntryUsed</handler>

                <!-- reset arp-entry used <address> -->
                <argument>
                    <name>!&lt;ip address&gt;</name>
                    <help>The CIDR style IP address</help>

                    <!-- reset arp-entry used <address> interface -->
                    <argument>
                        <name>interface</name>
                        <help>Reset ARP entry for specified interface</help>
                        <argument>
                            <name>!&lt;interface&gt;</name>
                            <help>The interface identifier</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Reset an ARP entry</help>
                            </argument>
                        </argument>
                    </argument> <!-- interface -->

                    <!-- reset arp-entry used <address> vlan -->
                    <argument>
                        <name>vlan</name>
                        <help>Reset ARP entry for specified vlan</help>
                        <argument>
                            <name>!&lt;vlan&gt;</name>
                            <help>The vlan identifier</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Reset an ARP entry</help>
                            </argument>
                        </argument>
                    </argument> <!-- vlan -->
                </argument> <!-- <address> -->
            </command>  <!-- reset arp-entry used -->
          
        </command>  <!-- reset arp-entry -->

    </command>  <!-- reset -->


    <!-- set -->
    <command>
        <name>set</name>

        <!-- set router -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>router</name>
            <help>Set a router related property</help>
            <handler>tpHandleSetRouterAttribute</handler>

            <!-- set router mac -->
            <argument>
                <name>mac</name>
                <help>Set the router MAC address</help>
                <argument>
                    <name>!&lt;mac address&gt;</name>
                    <help>The router MAC address</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Set the router MAC address</help>
                    </argument>
                </argument>
            </argument> <!-- mac -->

            <!-- set router ttl-disposal -->
            <argument>
                <name>ttl-disposal</name>
                <help>Set the TTL disposal action</help>

                <argument>
                    <name>drop-all</name>
                    <help>Drop all frames with TTL 1 or 0.</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Drop all frames with TTL 1 or 0.</help>
                    </argument>
                </argument>

                <argument>
                    <name>trap-icmp</name>
                    <help>Trap ICMP frames with TTL 1 or 0.</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Trap ICMP frames with TTL 1 or 0.</help>
                    </argument>
                </argument>

                <argument>
                    <name>trap-all</name>
                    <help>Trap all frames with TTL 1 or 0.</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Trap all frames with TTL 1 or 0.</help>
                    </argument>
                </argument>
            </argument> <!-- ttl-disposal -->

            <!-- set router trap-ip-opts -->
            <argument>
                <name>trap-ip-opts</name>
                <help>Specify whether to trap frames with IP options</help>

                <argument>
                    <name>enable</name>
                    <help>Trap frames with IP options.</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Trap frames with IP options.</help>
                    </argument>
                </argument>

                <argument>
                    <name>disable</name>
                    <help>Forward frames with IP options.</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Forward frames with IP options.</help>
                    </argument>
                </argument>
            </argument> <!-- trap-ip-opts -->

            <!-- set router virtual-mac -->
            <argument>
                <name>virtual-mac</name>
                <help>Set the virtual router MAC address</help>
                <argument>
                    <name>!&lt;mac address&gt;</name>
                    <help>
                        The virtual router MAC address (bottom 8-bits are
                        ignored)
                    </help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Set the virtual router MAC address</help>
                    </argument>
                </argument>
            </argument> <!-- virtual-mac -->

        </command> <!-- set router -->
    </command> <!-- set -->


    <!-- show -->
    <command>
        <name>show</name>

        <!-- show arp-entry -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>arp-entry</name>
            <help>Show an ARP entry</help>
            <handler>tpHandleShowArpEntry</handler>

            <!-- show arp-entry <address> -->
            <argument>
                <name>!&lt;ip address&gt;</name>
                <help>The CIDR style IP address</help>

                <!-- show arp-entry <address> interface -->
                <argument>
                    <name>interface</name>
                    <help>Display ARP entry for specified interface</help>
                    <argument>
                        <name>!&lt;interface&gt;</name>
                        <help>The interface identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Display an ARP entry</help>
                        </argument>
                    </argument>
                </argument> <!-- interface -->

                <!-- show arp-entry <address> vlan -->
                <argument>
                    <name>vlan</name>
                    <help>Display ARP entry for specified vlan</help>
                    <argument>
                        <name>!&lt;vlan&gt;</name>
                        <help>The vlan identifier</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Display an ARP entry</help>
                        </argument>
                    </argument>
                </argument> <!-- vlan -->

            </argument> <!-- <address> -->

            <!-- show arp-entry all -->
            <argument>
                <name>all</name>
                <help>Show all ARP entries</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Show all ARP entries</help>
                </argument>
            </argument>

        </command> <!-- show arp-entry -->

        <!-- show interface -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>interface</name>
            <help>Show router interfaces</help>
            <command>
                <name>all</name>
                <help>Show all router interfaces</help>
                <handler>tpHandleShowInterface</handler>

                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Show all router interfaces</help>
                </argument>
            </command>
        </command> <!-- show interface -->

        <!-- show route -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>
            <name>route</name>
            <help>Show routes</help>
            <handler>tpHandleShowRoute</handler>

            <argument>
                <name>physical</name>
                <help>
                    Show the subset of routes belonging to the physical router
                </help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>
                        Show the subset of routes belonging to the physical
                        router
                    </help>
                </argument>
            </argument>

            <argument>
                <name>!&lt;vrid&gt;</name>
                <help>Virtual router ID in the range 1-255</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>
                        Show the subset of unicast routes belonging to the
                        virtual router
                    </help>
                </argument>
            </argument>

            <argument>
                <name>all</name>
                <help>Show all routes</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Show all routes</help>
                </argument>
            </argument>
        </command> <!-- show route -->

        <!-- show router -->
        <command>
            <filter type="family">
                <deny>FM2000</deny>
            </filter>

            <name>router</name>
            <help>Display router related properties</help>
            <handler>tpHandleShowRouter</handler>

            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show router properties</help>
            </argument>
        </command>  <!-- end show router -->

    </command> <!-- show -->

</config>

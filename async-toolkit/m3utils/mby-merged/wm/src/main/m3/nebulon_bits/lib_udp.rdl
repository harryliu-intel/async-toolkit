    type      = string;
    default   = "";
};

// Parameterized expression for field Range 
// Syntax: <MSB parameterized expression:LSB parameterized expression>
// Example1: ParametersFieldRange = "%LSB%+FieldSizePrm-1:%LSB%"; 
// Example2: ParametersFieldRange = "%MSB%:%MSB%-FieldSizePrm+1"; 
property ParametersFieldSize {
    component = field;
    type      = string;
    default   = "";
};


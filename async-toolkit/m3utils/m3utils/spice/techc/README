How to run this code, example:

nbfeeder start --work-area /netbatch/mnystroe/mnystroe_`hostname`_feeder/ --name mnystroe_`hostname`_feeder --server

# --server for heavily loaded feeders, per Noam Ambar

cd m3utils/spice/techc # this directory
mkdir work 
cd work
../src/maketask.sh -quick
nbtask load latest.task
nbstatus tasks
cd nb.latest
cat *.run/measure.dat > all.dat

# basic version
../../digest/AMD64_LINUX/techdigest -f all.dat -o graphs -C -se 1e15

# frequency density version
../../digest/AMD64_LINUX/techdigest -f all.dat -o graphs -C -se 1e15 -A Cels i0s 4.48e-12 -A Cels i0m 5.04e-12

cd graphs
gnuplot
set style data linespoints
plot "n5_tt_ulvtll_xor_dyn_xa_50.dat"


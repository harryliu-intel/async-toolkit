    ValRandomize = true;
  } WB_FIFO_PERR[4:4] = 1'h00;
  field {
    AccessType = "RO/V";
    desc = "
            Interrupt pending bit, for interrupt of triggers recorded in TRIGGER_IP register.
           ";
    ValRandomize = true;
  } TRIGGER[3:3] = 1'h00;
  field {
    AccessType = "RO/V";
    desc = "
            Interrupt pending bit, for interrupt of MA_TABLE lookup recorded in MA_TCAM_ERR_IP register.
           ";
    ValRandomize = true;
  } TCAM_ERR[2:2] = 1'h00;
  field {
    AccessType = "RO/V";
    desc = "
            Interrupt pending bit, for interrupt of MA_TABLE lookup recorded in ENTRY_COUNT_IP register.
           ";
    ValRandomize = true;
  } ENTRY_COUNT[1:1] = 1'h00;
  field {
    AccessType = "RW/1C/V";
    desc = "
            Interrupt pending bits, for uncorrectable error set in shell_ctrl.
           ";
    ValRandomize = true;
  } SHELL_CTRL_U_ERR[0:0] = 1'h00;
};


#todo+=(seq_ulvt/100)
#todo+=(spcl_ulvt/200)
#todo+=(clk_ulvt/200)

# LVT bundles
#todo+=(base_lvt/100)
#todo+=(dsbase_lvt/100)
#todo+=(dsimain_lvt/200)
#todo+=(dsseq_lvt/200)
#todo+=(seq_lvt/200)

#launch_corner tttt ${millivolts}  ${celsius} max tttt_100c_tttt_cmax
launch_corner tttt ${millivolts}  ${celsius} max 100c_tttt_cmax
[[ -n $import_only ]] && exit

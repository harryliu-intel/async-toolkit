# show port lldp <port> config|local|remote|counters

 1,  "show",             "c",    "m",    "",                               "Show a property"
 2,  "port",             "c",    "m",    "",                               "Show a port related property or state"
 3,  "lldp",             "c",    "m",    "tpHandleShowLldpPort",           "Show LLDP port information"
 4,  "port",             "v",    "m",    "",                               "Show LLDP port information (enter port number, list, range)"

 5,  "config",           "t",    "m",    "",                               "Show port configuration information"
 5,  "local",            "t",    "m",    "",                               "Show port local-data information"
 5,  "remote",           "t",    "m",    "",                               "Show port remote-data information"
 5,  "counters",         "t",    "m",    "",                               "Show port counters information"

# show switch lldp config|local

 2,  "switch",           "c",    "m",    "",                               "Show a switch related property or state"

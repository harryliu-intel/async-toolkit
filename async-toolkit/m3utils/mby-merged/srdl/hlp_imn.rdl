reg sw_test_and_set_r {
  shared;
  HandCoded = true;
  name = "Software Test and Set";
  desc = "Software Test and Set, used as a semaphore.

          The mask is used to exclude some bits from the operation. If the unmasked register
          content is zero and the new value is not zero, then the new value is loaded into the register unmasked bits.
          If the new value is zero, then this new value value is loaded into the register unmasked bits regardless of
          its current content.

          The operation is atomic and the pseudo code is:

          if ( (SW_TEST_AND_SET.Data & ~Mask) == 0 || (Data & ~Mask) == 0 ) 
          begin

            SW_TEST_AND_SET.Data = (SW_TEST_AND_SET.Data & Mask) | (Data & ~Mask)

            SW_TEST_AND_SET.Mask = Mask 

          end
  ";
  regwidth = 64;
  accesswidth = 64;
  Security_PolicyGroup  = <%=$Security_Props{'HLP_PG1'}%>;
  Security_ReadAccess_Str = <%=$Security_Props{'HLP_PG1_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'HLP_PG1_WAC_AGENTS'}%>;
  field {
    AccessType = "RW";
    desc = " Defines bits to keep unchanged in the register.  If mask bit i is set to 1, then the data bit i is not going to be affected during the TEST and SET operation. ";
    ValRandomize = true;
  } MASK[31:16] = 16'h0;
  field {
    AccessType = "RW";
    desc = "New value to write";
    ValRandomize = true;
  } DATA[15:0] = 16'h0;
};




module standard.pseudorandom;

/** Generate random data for testing **/
define GENERATE_PSEUDORANDOM(int N,W) <: standard.base.NO_RESET_CELL {
  csp {
    string s="  int data[0.." + (N-1) + "]=\n    {";
    <i:N:
      s+=hex(random(W));
      [ i==N-1 -> s+="};\n     "
      []else -> s+=",\n     "
      ]
    >;
    print(s)
  }
}


//------------------------------------------------------------------------------
// User Defined Properties (UDP) Library
//
// This contains all of the UDPs that are used globally throughout all register
// descriptions.  UDP should not be declared anywhere else.
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Required Address Map UDPs
//------------------------------------------------------------------------------
//Generate Code
property Space {
    component = addrmap;
    type      = string;
    default   = "";
};

//Generate Code
property BaseAddress {
    component = addrmap;
    type      = string;
    default   = "";
};


//Generate Code
//For HSD 871497
property USER_SPECIFIED_UDP {
    component = addrmap;
    type      = string;
    default   = "";
};

//For HSD 889327
property ModuleName {
    component = addrmap | reg | field;
    type      = string;
    default   = "";
};

//For HSD 912981 dmeka
property RTLPriority {
    component = reg | field;
    type      = string;
    default   = "";
};

//------------------------------------------------------------------------------
// Required Register UDPs
//------------------------------------------------------------------------------
//Generate Code
property AliasAddress {
    component = reg | addrmap;
    type      = string;
    default   = "";
};


//Generate Code`
// this UDP defines the power well for both reg & field level:
property PowerWell {
    component = reg | field;
    type      = string;
    default   = "";
};


//------------------------------------------------------------------------------
// Required Field UDPs
//------------------------------------------------------------------------------
//Generate Code
property AccessType {
    component = field; 
    type      = string;
    default   = "";
};


//Generate Code
// FieldDefaultValue is used to define Field Default Value override
//   - FieldDefaultValue="X";
property FieldDefaultValue {
    component = field;
    type      = string;
    default   = "";
};

//Generate Code
property ValLegalValues {
    component = field;
    type      = string;
    default   = "";
};

//Generate Code
property ValRandomize {
    component = field;
    type      = boolean;
    default   = false;
};

//Generate Code
// GLS RTL Signal path
property ValGLSSignal {
    component = field | addrmap;
    type      = string;
    default   = "";
};

//Generate Code
property ValRTLSignal {
    component = reg | field | addrmap | regfile;
    type      = string;
    default   = "";
};


//Generate Code
// specify one bit register field that serve as the locking Key for the corresponding lockable field. The key field can be in the same register or different register
property LockKeyField {
    component = field;
    type      = string; 
    default   = "";
};

//Generate Code
// specify whether a key value of 0 or 1 will cause the corresponding field to lock and behaves as RO. Default value is 1 if not specified.
property LockKeyValue {
    component = field;
    type      = number; 
    default   = 1;       //Changed to 1 (from true) per HSD 5212157
};


//Generate Code
//Added per HSD 896535  for power retention awareness
property StateRetention {
  component = field;
  type = boolean;
  default = false;
};


//Generate Code
  //Must specify PGD with StateRetention
property PGD {
  component = field;
  type = string;
  default = "";
};

//Generate Code
//Added per HSD 913197
property ResetSignal {
    component = reg | field;
    type      = string;
    default   = "";
};


//Generate Code
//Added per HSD 5079148

 property IsIndirect {
   component = reg;
   type = boolean;
   default = false;
 };

//Generate Code
property DefaultValRTLSignal {
    component = addrmap;

module aliasing;

define P()()
{

  csp {
    function f(int +-a[0..0], +-b[0..0]) =
      (a[0] = 1;
       b[1] = 2
       );

    int x[0..1];

    print("x[0] = " + x[0] + " x[1] = " + x[1]);

    f(x,x);
    

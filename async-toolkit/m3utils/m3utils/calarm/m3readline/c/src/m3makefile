% we should do this all through cm3cfg

if SYSTEM_LIBS contains "READLINE"
	import_sys_lib("READLINE")
end

if equal(TARGET, "LINUXLIBC6")
        import_lib ("readline","/usr/lib")
end

if equal(TARGET, "PPC_DARWIN")
	import_lib ("ncurses","/usr/lib")
end

import ("libm3")

c_source ("program")

C_program("readlinefe")

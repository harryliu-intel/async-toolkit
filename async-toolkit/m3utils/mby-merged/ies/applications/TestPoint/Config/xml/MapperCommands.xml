<config>

<!-- show -->
<command>
    <name>show</name>
    <help>Show a property</help>

    <!-- show mapper -->
    <command>
        <filter type="family">
           <deny>FM2000</deny>
        </filter>
        <name>mapper</name>
        <help>Show a Mapper entry</help>
        <handler>tpHandleShowMapper</handler>

        <!-- show mapper source -->
        <argument>
            <name>source</name>
            <help>Show the Source Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the Source Mapper</help>
            </argument>
        </argument>  <!-- end show mapper source -->

        <!-- show mapper protocol -->
        <argument>
            <name>protocol</name>
            <help>Show the Protocol Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the Protocol Mapper</help>
            </argument>
        </argument>  <!-- end show mapper protocol -->

        <!-- show mapper l4src -->
        <argument>
            <name>l4src</name>
            <help>Show the L4 Source Port Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the L4 Source Port Mapper</help>
            </argument>
        </argument>  <!-- end show mapper l4src -->

        <!-- show mapper l4dst -->
        <argument>
            <name>l4dst</name>
            <help>Show the L4 Destination Port Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the L4 Destination Port Mapper</help>
            </argument>
        </argument>  <!-- end show mapper l4dst -->

        <!-- show mapper mac -->
        <argument>
            <name>mac</name>
            <help>Show the MAC Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the MAC Mapper</help>
            </argument>
        </argument>  <!-- end show mapper mac -->

        <!-- show mapper ethtype -->
        <argument>
            <name>ethtype</name>
            <help>Show the Ethernet Type Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the Ethernet Type Mapper</help>
            </argument>
        </argument>  <!-- end show mapper ethtype -->

        <!-- show mapper iplength -->
        <argument>
            <name>iplength</name>
            <help>Show the IP Length Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the IP Length Mapper</help>
            </argument>
        </argument>  <!-- end show mapper iplength -->

        <!-- show mapper ipaddr -->
        <argument>
            <name>ipaddr</name>
            <help>Show the IP Address Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the IP Address Mapper</help>
            </argument>
        </argument>  <!-- end show mapper ipaddr -->

        <!-- show mapper vlan -->
        <argument>
            <name>vlan</name>
            <help>Show the vlan ID Mapper</help>
            <argument>
                <name>!&lt;cr&gt;</name>
                <help>Show the vlan ID Mapper</help>
            </argument>
        </argument>  <!-- end show mapper vlan -->

    </command>  <!-- end show mapper -->

</command>  <!-- end show -->

<!-- del -->
<command>
    <name>del</name>
    <help>Delete MAC address, VLANs, LAGs, or ports from VLANs, or counters from VLANs or LAGs</help>

    <!-- del mapper -->
    <command>
        <filter type="family">
           <deny>FM2000</deny>
        </filter>
        <name>mapper</name>
        <help>Delete a Mapper entry</help>
        <handler>tpHandleDelMapper</handler>

        <!-- del mapper source -->
        <argument>
            <name>source</name>
            <help>Delete a Source Mapper Entry</help>
            <!-- del mapper source <port> -->
            <argument>
                <name>!&lt;port&gt;</name>
                <help>Logical port to remove from the Mapper</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete this logical port from the Mapper</help>
                </argument>
            </argument> <!-- <port> -->
        </argument>  <!-- end del mapper source -->

        <!-- del mapper protocol -->
        <argument>
            <name>protocol</name>
            <help>Delete a Protocol Mapper Entry</help>
            <!-- del mapper protocol <protocol> -->
            <argument>
                <name>!&lt;protocol&gt;</name>
                <help>protocol with optional mask (e.g. 0x12/0xff)</help>
                <!-- del mapper protocol <protocol> <map> -->
                <argument>
                    <name>!&lt;map&gt;</name>
                    <help>Mapped value to delete</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Delete this protocol Mapper Entry</help>
                    </argument>
                </argument> <!-- <map> -->
            </argument> <!-- <protocol> -->
        </argument>  <!-- end del mapper protocol -->

        <!-- del mapper l4src -->
        <argument>
            <name>l4src</name>
            <help>Delete a L4 Source Port Mapper Entry</help>
            <!-- del mapper l4src <start> -->
            <argument>
                <name>!&lt;start&gt;</name>
                <help>L4 port that starts the range of ports being mapped</help>
                <!-- del mapper l4src <start> <end> -->
                <argument>
                    <name>!&lt;end&gt;</name>
                    <help>L4 port that ends the range of ports being mapped</help>
                    <!-- del mapper l4src <start> <end> <protocol> <map> -->
                    <argument>
                        <name>!&lt;protocol&gt;</name>
                        <help>Protocol or mapped protocol associated with this L4 port range</help>
                        <argument>
                            <name>!&lt;map&gt;</name>
                            <help>Mapped value to delete</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Delete this L4 source port Mapper Entry</help>
                            </argument>
                        </argument> <!-- <map> -->
                    </argument> <!-- <protocol> -->
                </argument> <!-- <end> -->
            </argument> <!-- <start> -->
        </argument>  <!-- end del mapper l4src -->

        <!-- del mapper l4dst -->
        <argument>
            <name>l4dst</name>
            <help>Delete a L4 Destination Port Mapper Entry</help>
            <!-- del mapper l4dst <start> -->
            <argument>
                <name>!&lt;start&gt;</name>
                <help>L4 port that starts the range of ports being mapped</help>
                <!-- del mapper l4dst <start> <end> -->
                <argument>
                    <name>!&lt;end&gt;</name>
                    <help>L4 port that ends the range of ports being mapped</help>
                    <!-- del mapper l4dst <start> <end> <protocol> <map> -->
                    <argument>
                        <name>!&lt;protocol&gt;</name>
                        <help>Protocol or mapped protocol associated with this L4 port range</help>
                        <argument>
                            <name>!&lt;map&gt;</name>
                            <help>Mapped value to delete</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Delete this L4 destination port Mapper Entry</help>
                            </argument>
                        </argument> <!-- <map> -->
                    </argument> <!-- <protocol> -->
                </argument> <!-- <end> -->
            </argument> <!-- <start> -->
        </argument>  <!-- end del mapper l4dst -->

        <!-- del mapper mac -->
        <argument>
            <name>mac</name>
            <help>Delete a MAC Mapper Entry</help>
            <!-- del mapper mac source -->
            <argument>
                <name>source</name>
                <help>Delete a Source MAC Mapper Entry</help>
                <!-- del mapper mac source <mac> -->
                <argument>
                    <name>!&lt;mac&gt;</name>
                    <help>MAC address and optional mask (e.g. 00:15:ed:00:00:00/ff:ff:ff:00:00:00)</help>
                    <!-- del mapper mac source <mac> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to delete</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete this MAC Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <mac> -->
            </argument> <!-- source -->

            <!-- del mapper mac destination -->
            <argument>
                <name>destination</name>
                <help>Delete a Destination MAC Mapper Entry</help>
                <!-- del mapper mac destination <mac> -->
                <argument>
                    <name>!&lt;mac&gt;</name>
                    <help>MAC address and optional mask (e.g. 00:15:ed:00:00:00/ff:ff:ff:00:00:00)</help>
                    <!-- del mapper mac destination <mac> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to delete</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete this MAC Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <mac> -->
            </argument> <!-- destination -->
        </argument>  <!-- end del mapper mac -->

        <!-- del mapper ethtype -->
        <argument>
            <name>ethtype</name>
            <help>Delete an EtherType Mapper Entry</help>
            <!-- del mapper ethtype <ethtype> -->
            <argument>
                <name>!&lt;ethtype&gt;</name>
                <help>EtherType with optional mask (e.g. 0x0811/0xfff)</help>
                <!-- del mapper ethtype <ethtype> <map> -->
                <argument>
                    <name>!&lt;map&gt;</name>
                    <help>Mapped value to delete</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Delete this EtherType Mapper Entry</help>
                    </argument>
                </argument> <!-- <map> -->
            </argument> <!-- <ethtype> -->
        </argument>  <!-- end del mapper ethtype -->

        <!-- del mapper iplength -->
        <argument>
            <name>iplength</name>
            <help>Delete an IP Length Mapper Entry</help>
            <!-- del mapper iplength <start> -->
            <argument>
                <name>!&lt;start&gt;</name>
                <help>IP length that starts the range of length being mapped</help>
                <!-- del mapper iplength <start> <end> -->
                <argument>
                    <name>!&lt;end&gt;</name>
                    <help>IP length that ends the range of length being mapped</help>
                    <!-- del mapper iplength <start> <end> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to delete</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete this IP Length Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <end> -->
            </argument> <!-- <start> -->
        </argument>  <!-- end del mapper iplength -->

        <!-- del mapper ipaddr -->
        <argument>
            <name>ipaddr</name>
            <help>Delete an IP Address Entry</help>
            <!-- del mapper ipaddr source -->
            <argument>
                <name>source</name>
                <help>Delete a Source IP Address Entry</help>
                <!-- del mapper ipaddr source <ip> -->
                <argument>
                    <name>!&lt;ip&gt;</name>
                    <help>IP address and optional mask (e.g. 192.0.2.0/24)</help>
                    <!-- del mapper ipaddr source <ip> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to delete</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete this Source IP Address Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <ip> -->
            </argument> <!-- source -->

            <!-- del mapper ipaddr destination -->
            <argument>
                <name>destination</name>
                <help>Delete a Destination IP Address Entry</help>
                <!-- del mapper ipaddr destination <ip> -->
                <argument>
                    <name>!&lt;ip&gt;</name>
                    <help>IP address and optional mask (e.g. 192.0.2.0/24)</help>
                    <!-- del mapper ipaddr destination <ip> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to delete</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Delete this Destination IP Address Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <ip> -->
            </argument> <!-- destination -->
        </argument>  <!-- end del mapper ipaddr -->

        <!-- del mapper vlan -->
        <argument>
            <name>vlan</name>
            <help>Delete a vlan ID Mapper Entry</help>
            <!-- del mapper vlan <vlanId> -->
            <argument>
                <name>!&lt;vlanId&gt;</name>
                <help>vlan ID to remove from the Mapper</help>
                <argument>
                    <name>!&lt;cr&gt;</name>
                    <help>Delete this vlan ID from the Mapper</help>
                </argument>
            </argument> <!-- <vlanId> -->
        </argument>  <!-- end del mapper vlan -->
    </command>  <!-- end del mapper -->
</command>  <!-- end del -->

<!-- add -->
<command>
    <name>add</name>
    <help>Add mac addresses to MA table, or ports to VLANs or LAGs</help>

    <!-- add mapper -->
    <command>
        <filter type="family">
           <deny>FM2000</deny>
        </filter>
        <name>mapper</name>
        <help>Add a Mapper entry</help>
        <handler>tpHandleAddMapper</handler>

        <!-- add mapper source -->
        <argument>
            <name>source</name>
            <help>Add a Source Mapper Entry</help>
            <!-- add mapper source <port> -->
            <argument>
                <name>!&lt;port&gt;</name>
                <help>Logical port to add into the Mapper</help>
                <!-- add mapper source <port> <map> -->
                <argument>
                    <name>!&lt;map&gt;</name>
                    <help>Mapped value to add</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Add this source Mapper Entry</help>
                    </argument>
                </argument> <!-- <map> -->
            </argument> <!-- <port> -->
        </argument>  <!-- end add mapper source -->

        <!-- add mapper protocol -->
        <argument>
            <name>protocol</name>
            <help>Add a Protocol Mapper Entry</help>
            <!-- add mapper protocol <protocol> -->
            <argument>
                <name>!&lt;protocol&gt;</name>
                <help>protocol with optional mask (e.g. 0x12/0xff)</help>
                <!-- add mapper protocol <protocol> <map> -->
                <argument>
                    <name>!&lt;map&gt;</name>
                    <help>Mapped value to add</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Add this protocol Mapper Entry</help>
                    </argument>
                </argument> <!-- <map> -->
            </argument> <!-- <protocol> -->
        </argument>  <!-- end add mapper protocol -->

        <!-- add mapper l4src -->
        <argument>
            <name>l4src</name>
            <help>Add a L4 Source Port Mapper Entry</help>
            <!-- add mapper l4src <start> -->
            <argument>
                <name>!&lt;start&gt;</name>
                <help>L4 port that starts the range of ports being mapped</help>
                <!-- add mapper l4src <start> <end> -->
                <argument>
                    <name>!&lt;end&gt;</name>
                    <help>L4 port that ends the range of ports being mapped</help>
                    <!-- add mapper l4src <start> <end> <protocol> <map> -->
                    <argument>
                        <name>!&lt;protocol&gt;</name>
                        <help>Protocol or mapped protocol associated with this L4 port range</help>
                        <argument>
                            <name>!&lt;map&gt;</name>
                            <help>Mapped value to add</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Add this L4 source port Mapper Entry</help>
                            </argument>
                        </argument> <!-- <map> -->
                    </argument> <!-- <protocol> -->
                </argument> <!-- <end> -->
            </argument> <!-- <start> -->
        </argument>  <!-- end add mapper l4src -->

        <!-- add mapper l4dst -->
        <argument>
            <name>l4dst</name>
            <help>Add a L4 Destination Port Mapper Entry</help>
            <!-- add mapper l4dst <start> -->
            <argument>
                <name>!&lt;start&gt;</name>
                <help>L4 port that starts the range of ports being mapped</help>
                <!-- add mapper l4dst <start> <end> -->
                <argument>
                    <name>!&lt;end&gt;</name>
                    <help>L4 port that ends the range of ports being mapped</help>
                    <!-- add mapper l4dst <start> <end> <protocol> <map> -->
                    <argument>
                        <name>!&lt;protocol&gt;</name>
                        <help>Protocol or mapped protocol associated with this L4 port range</help>
                        <argument>
                            <name>!&lt;map&gt;</name>
                            <help>Mapped value to add</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Add this L4 destination port Mapper Entry</help>
                            </argument>
                        </argument> <!-- <map> -->
                    </argument> <!-- <protocol> -->
                </argument> <!-- <end> -->
            </argument> <!-- <start> -->
        </argument>  <!-- end add mapper l4dst -->

        <!-- add mapper mac -->
        <argument>
            <name>mac</name>
            <help>Add a MAC Mapper Entry</help>
            <!-- add mapper mac source -->
            <argument>
                <name>source</name>
                <help>Add a Source MAC Mapper Entry</help>
                <!-- add mapper mac source <mac> -->
                <argument>
                    <name>!&lt;mac&gt;</name>
                    <help>MAC address and optional mask (e.g. 00:15:ed:00:00:00/ff:ff:ff:00:00:00)</help>
                    <!-- add mapper mac source <mac> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to add</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add this MAC Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <mac> -->
            </argument> <!-- source -->

            <!-- add mapper mac destination -->
            <argument>
                <name>destination</name>
                <help>Add a Destination MAC Mapper Entry</help>
                <!-- add mapper mac destination <mac> -->
                <argument>
                    <name>!&lt;mac&gt;</name>
                    <help>MAC address and optional mask (e.g. 00:15:ed:00:00:00/ff:ff:ff:00:00:00)</help>
                    <!-- add mapper mac destination <mac> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to add</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add this MAC Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <mac> -->
            </argument> <!-- destination -->
        </argument>  <!-- end add mapper mac -->

        <!-- add mapper ethtype -->
        <argument>
            <name>ethtype</name>
            <help>Add an EtherType Mapper Entry</help>
            <!-- add mapper ethtype <ethtype> -->
            <argument>
                <name>!&lt;ethtype&gt;</name>
                <help>EtherType with optional mask (e.g. 0x0811/0xfff)</help>
                <!-- add mapper ethtype <ethtype> <map> -->
                <argument>
                    <name>!&lt;map&gt;</name>
                    <help>Mapped value to add</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Add this EtherType Mapper Entry</help>
                    </argument>
                </argument> <!-- <map> -->
            </argument> <!-- <ethtype> -->
        </argument>  <!-- end add mapper ethtype -->

        <!-- add mapper iplength -->
        <argument>
            <name>iplength</name>
            <help>Add an IP Length Mapper Entry</help>
            <!-- add mapper iplength <start> -->
            <argument>
                <name>!&lt;start&gt;</name>
                <help>IP length that starts the range of length being mapped</help>
                <!-- add mapper iplength <start> <end> -->
                <argument>
                    <name>!&lt;end&gt;</name>
                    <help>IP length that ends the range of length being mapped</help>
                    <!-- add mapper iplength <start> <end> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to add</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add this IP Length Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <end> -->
            </argument> <!-- <start> -->
        </argument>  <!-- end add mapper iplength -->

        <!-- add mapper ipaddr -->
        <argument>
            <name>ipaddr</name>
            <help>Add an IP Address Entry</help>
            <!-- add mapper ipaddr source -->
            <argument>
                <name>source</name>
                <help>Add a Source IP Address Entry</help>
                <!-- add mapper ipaddr source <ip> -->
                <argument>
                    <name>!&lt;ip&gt;</name>
                    <help>IP address and optional mask (e.g. 192.0.2.0/24)</help>
                    <!-- add mapper ipaddr source <ip> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to add</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add this Source IP Address Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <ip> -->
            </argument> <!-- source -->

            <!-- add mapper ipaddr destination -->
            <argument>
                <name>destination</name>
                <help>Add a Destination IP Address Entry</help>
                <!-- add mapper ipaddr destination <ip> -->
                <argument>
                    <name>!&lt;ip&gt;</name>
                    <help>IP address and optional mask (e.g. 192.0.2.0/24)</help>
                    <!-- add mapper ipaddr destination <ip> <map> -->
                    <argument>
                        <name>!&lt;map&gt;</name>
                        <help>Mapped value to add</help>
                        <argument>
                            <name>!&lt;cr&gt;</name>
                            <help>Add this Destination IP Address Mapper Entry</help>
                        </argument>
                    </argument> <!-- <map> -->
                </argument> <!-- <ip> -->
            </argument> <!-- destination -->
        </argument>  <!-- end add mapper ipaddr -->

        <!-- add mapper vlan -->
        <argument>
            <name>vlan</name>
            <help>Add a vlan ID Mapper Entry</help>
            <!-- add mapper vlan <vlanId> -->
            <argument>
                <name>!&lt;vlanId&gt;</name>
                <help>vlan ID to add into the Mapper</help>
                <!-- add mapper vlan <vlanId> <map> -->
                <argument>
                    <name>!&lt;map&gt;</name>
                    <help>Mapped value to add</help>
                    <argument>
                        <name>!&lt;cr&gt;</name>
                        <help>Add this vlan ID Mapper Entry</help>
                    </argument>
                </argument> <!-- <map> -->
            </argument> <!-- <vlanId> -->
        </argument>  <!-- end add mapper vlan -->
    </command>  <!-- end add mapper -->
</command>  <!-- end add -->

</config>

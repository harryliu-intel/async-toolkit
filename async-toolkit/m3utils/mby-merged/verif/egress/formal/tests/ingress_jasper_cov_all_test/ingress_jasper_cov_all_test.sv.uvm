


import ovm_pkg::*;
import sla_pkg::*;
import egress_env_pkg::*;

`include "ovm_macros.svh"
`include "sla_macros.svh"

class egress_jasper_cov_all_test extends ovm_test;
  
  `ovm_component_utils(egress_jasper_cov_all_test)
    
  egress_env env;
  
  function new (string name="egress_jasper_cov_all_test", ovm_component parent=null);
    super.new (name, parent);
  endfunction // new

   function void build();
    ovm_report_info(get_full_name(),"Build", OVM_LOG);
    env = egress_env::type_id::create("env",this);
     
   endfunction // void

   function void connect();
     super.connect();

// START IOSF_NOT_PRESENT
     env.set_test_phase_type("env", "HARD_RESET_PHASE", "egress_hard_reset_seq");
     env.set_test_phase_type("env", "CONFIG_PHASE", "egress_pci_config_seq");
// END IOSF_NOT_PRESENT
   endfunction // void
  
  
endclass // my_test

module egress_jasper_cov_all_test ();

import xvm_pkg::*;

initial begin
if ($value$plusargs("UVM_TESTNAME=%s", testname  ))
    xvm_pkg::run_test("","", xvm::EOP_UVM);
  else
    run_test("");

end
endmodule // egress_jasper_cov_all_test


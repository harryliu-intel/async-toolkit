    desc = "
            Selects hash rotation A (0) or rotation B (1) for filtering.
           ";
    ValRandomize = true;
  } HASH_ROTATION[8:8] = 1'h00;
  field {
    AccessType = "RW";
    desc = "
            Index of this port in the link aggregation port, must be between 
            0 and LAG_CFG:LagSizeâ€”1.
           ";
    ValRandomize = true;
  } INDEX[7:4] = 4'h00;
  field {
    AccessType = "RW";
    desc = "
            Defines the number of ports in the link aggregation group. The 
            value 0 means 16.
           ";
    ValRandomize = true;
  } LAG_SIZE[3:0] = 4'h00;
};


reg fwd_port_cfg_1_r {

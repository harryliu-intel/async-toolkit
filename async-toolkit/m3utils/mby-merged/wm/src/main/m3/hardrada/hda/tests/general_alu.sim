( define ( general_alu x y op list_of_fns )
	( cond ( ( or ( < op 0 ) ( null? list_of_fns ) ) #f )
	( ( = op 0 ) ( ( car list_of_fns ) x y ) )
	( else ( alu x y ( - op 1 ) ( cdr list_of_fns ) ) )
	)
)


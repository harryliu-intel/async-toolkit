reg fwd_sys_cfg_1_r {
  Security_PolicyGroup     = <%=$Security_Props{'MBY_PG1'}%>;
  Security_ReadAccess_Str  = <%=$Security_Props{'MBY_PG1_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'MBY_PG1_WAC_AGENTS'}%>;
  shared;
  name = "Layer 2 configuration";
  desc = "Miscellaneous configuration related to L2 forwarding behaviors.";
  regwidth = 64;
  accesswidth = 64;
  field {
    AccessType = "RW";
    desc = "
            Indicate whether the 4b action code is stored in hte packet metadata. 
            When set to 1, the 4b action code is stored inside bits 195:156 of metadata, 
            which is the upper 4b of the flags field. in addition, a 1b presence flag 
            is stored in bit 155. When set to 0, no change in metadata occurs.
           ";
    ValRandomize = true;
  } STORE_TRAP_ACTION[4:4] = 1'h01;
  field {
    AccessType = "RW";
    desc = "
            Drop MAC CTRL Ethertype 0x8808. This has no effect on PAUSE 
            processing.
           ";
    ValRandomize = true;
  } DROP_MAC_CTRL_ETHERTYPE[3:3] = 1'h01;
  field {
    AccessType = "RW";
    desc = "
            Drop any frame with an invalid SMAC of all zeros (00:00:00:00:00:00), 
            all ones (FF:FF:FF:FF:FF:FF) or multicast (bit 40 is a 1) and do not cause MAC learning.
           ";
    ValRandomize = true;
  } DROP_INVALID_SMAC[2:2] = 1'h01;
  field {
    AccessType = "RW";
    desc = "
            If set to 1, then trapping and logging can both occur for a given 
            frame. If set to 0, then trapping will take precedence over logging.
           ";
    ValRandomize = true;
  } ENABLE_TRAP_PLUS_LOG[1:1] = 1'h01;
  field {
    AccessType = "RW";
    desc = "
            Defines disposition for frames that exceed MTU size allowed on the
            egress VLAN (only applicable for routed frames). If this bit is 
            set, then the frames that exceed the MTU size are trapped to the 
            CPU. If this bit is reset, then the frames are forwarded normally.
           ";
    ValRandomize = true;
  } TRAP_MTU_VIOLATIONS[0:0] = 1'h01;
};


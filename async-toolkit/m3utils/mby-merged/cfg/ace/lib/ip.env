set SETUP_SITE = `/usr/intel/bin/sitecode`

switch  ($SETUP_SITE)
	case iil :
		setenv IP_RELEASE /p/coe/ip
                setenv NBPOOL      iil_vp
                setenv NBQSLOT     SOC_ncc_reg
		setenv NBCLASS     SLES11
		modpath -n 1 -f -q /p/coe/tools/bin
		breaksw
	case fm :
		setenv IP_RELEASE /p/coe/ip
		modpath -n 1 -f -q /p/coe/tools/bin
		breaksw
	case ch :
		breaksw
	case png:
		breaksw
	case an :
		setenv IP_RELEASE /p/socenv/cents/common/archive_root/vlv/vlv_ip
		breaksw
	defauit :
		echo "$SETUP SITE is not yet supported"
endsw

<% use security; %>
<% my %Security_Props = security::GetSecurityInfo(); %>
`include "hlp_udp.rdl"

enum time_scale {
  T_100NS = 3'd0 { desc = "100 ns"; };
  T_1US   = 3'd1 { desc = "1 us"; };
  T_10US  = 3'd2 { desc = "10 us"; };
  T_100US = 3'd3 { desc = "100 us"; };
  T_1MS   = 3'd4 { desc = "1 ms"; };
  T_10MS  = 3'd5 { desc = "10 ms"; };
  T_100MS = 3'd6 { desc = "100 ms"; };
  T_1S    = 3'd7 { desc = "1 s"; };
};



enum boot_stage_enum {
  LEVEL0              =  2'd0 { desc = "HLP powered-downm"; };
  LEVEL1              =  2'd1 { desc = "HLP powered-up, Fuses Pulled (Memories Repaired), SBIOSF access to LSM only, MAC + Fabric in reset"; };
  LEVEL2              =  2'd2 { desc = "Level 1 + MAC logic out of reset"; };
  LEVEL3              =  2'd3 { desc = "Level 2 + Switch logic out of reset"; };
};

reg soft_reset_r {
  shared;
  HandCoded = true;
  name = "For Debug Only, Reserved.";
  desc = "Soft reset for mac and switch";
  regwidth = 64;
  accesswidth = 64;
  Security_PolicyGroup  = <%=$Security_Props{'HLP_PG3'}%>;
  Security_ReadAccess_Str = <%=$Security_Props{'HLP_PG3_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'HLP_PG3_WAC_AGENTS'}%>;

  field {
    AccessType = "RW";
    desc = "when set, causes the boot sequencer to proceed without having to wait for MACSIA_MEM_INIT_DONE";
    ValRandomize = true;
  } MASK_MACSIA_MEM_INIT_DONE[13:13] = 1'h0;

  field {
    AccessType = "RW";
    desc = "when set, causes the boot sequencer to proceed without having to wait for SWITCH_MEM_INIT_DONE";
    ValRandomize = true;
  } MASK_SWITCH_MEM_INIT_DONE[12:12] = 1'h0;

  field {
    AccessType = "RW";
    desc = "when set, causes the boot sequencer to proceed without having to wait for MACSIA_INIT_DONE";
    ValRandomize = true;
  } MASK_MACSIA_INIT_DONE[11:11] = 1'h0;

  field {
    AccessType = "RW";
    desc = "when set, causes the boot sequencer to proceed without having to wait for SWITCH_INIT_DONE";
    ValRandomize = true;
  } MASK_SWITCH_INIT_DONE[10:10] = 1'h0;

  field {
    AccessType = "RW";
    desc = "when set, causes the boot sequencer to proceed as if fuse acknowledgements have been received for functional, straps, repair and ip_ready";
    ValRandomize = true;
  } FORCE_FUSE_PHASE_DONE[9:9] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request to force blocking management access to mac/sia";
    ValRandomize = true;
  } REQ_MACSIA_LL_OFF[8:8] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request to force blocking management access to switch";
    ValRandomize = true;
  } REQ_SWITCH_LL_OFF[7:7] = 1'h0;

  field {
    AccessType = "RW";
    desc = "mask_ports_rst. When set, HLP ignores the assertion of ports_rst pin";
    ValRandomize = true;
  } MASK_PORTS_RST[6:6] = 1'h0;

  field {
    AccessType = "RW";
    desc = "mask_switch_rst. When set, HLP ignores the assertion of switch_rst pin";
    ValRandomize = true;
  } MASK_SWITCH_RST[5:5] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request reset of the deepest mac-clock reset";
    ValRandomize = true;
  } REQ_PG_POWERGOOD_RST[4:4] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request reset of the mac-clock memories";
    ValRandomize = true;
  } REQ_MAC_MEM_RST[3:3] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request reset of the mac-clock logic";
    ValRandomize = true;
  } REQ_MAC_RST[2:2] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request reset of the switch-clock memories";
    ValRandomize = true;
  } REQ_SWITCH_MEM_RST[1:1] = 1'h0;

  field {
    AccessType = "RW";
    desc = "request reset of the switch-clock logic";
    ValRandomize = true;
  } REQ_SWITCH_RST[0:0] = 1'h0;

};


reg device_status_r {
  shared;
  HandCoded = true;
  name = "device status";
  desc = "IMN root mgmt handler device status";
  regwidth = 64;
  accesswidth = 64;
  Security_PolicyGroup  = <%=$Security_Props{'HLP_PG2'}%>;
  Security_ReadAccess_Str = <%=$Security_Props{'HLP_PG2_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'HLP_PG2_WAC_AGENTS'}%>;
  
  field {
    encode = boot_stage_enum;
    AccessType = "RO/V";
    desc = "Defines auto_seqr's boot step.
      This indicates that the auto_seqr is transitioning from level N-1 to N, or N+1 to N, if defined, or is at level N.
      Used in conjunction with the other DEVICE_STATUS fields to indicate:
      * (0) Level 0 HLP powered-down 
	    * (1) Level 1 HLP powered-up, Fuses Pulled, Memories Repaired, SBIOSF access to LSM only MAC and Fabric in reset
	    * (2) Level 2 Level 1 and MAC logic out of reset
	    * (3) Level 3 Level 2 + Switch logic out of reset";
    ValRandomize = true;
  } BOOT_STAGE[14:13] = 2'h0;

  field {
    AccessType = "RO/V";
    desc = "latched indication that the functional fuses were pulled";
    ValRandomize = true;
  } LOAD_DONE[12:12] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "latched indication that the repair fuses were pulled";
    ValRandomize = true;
  } REPAIR_DONE[11:11] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "latched indication that an ip_ready message was sent";
    ValRandomize = true;
  } IP_READY[10:10] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the global_rst_done pin";
    ValRandomize = true;
  } GLOBAL_RST_DONE[9:9] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the memory init done signal for the mac and sia";
    ValRandomize = true;
  } MACSIA_MEM_DONE[8:8] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the memory init done signal for the switch";
    ValRandomize = true;
  } SWITCH_MEM_DONE[7:7] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the functional init done signal for the mac and sia";
    ValRandomize = true;
  } MACSIA_INIT_DONE[6:6] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the functional init done signal for the switch";
    ValRandomize = true;
  } SWITCH_INIT_DONE[5:5] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the mac local-loop enable request";
    ValRandomize = true;
  } MAC_LL_REQ[4:4] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the mac local-loop enable acknowledge";
    ValRandomize = true;
  } MAC_LL_ACK[3:3] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the switch local-loop enable request";
    ValRandomize = true;
  } SWITCH_LL_REQ[2:2] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the switch local-loop enable acknowledge";
    ValRandomize = true;
  } SWITCH_LL_ACK[1:1] = 1'h0;

  field {
    AccessType = "RO/V";
    desc = "status of the hlp_ready pin";
    ValRandomize = true;
  } HLP_READY[0:0] = 1'h0;

};


// LSM_CFG

reg lsm_cfg_r {
  shared;
  HandCoded = true;
  name = "LSM Configuration";
  desc = "LSM Configuration";
  Security_PolicyGroup = <%=$Security_Props{'HLP_PG1'}%>;
  Security_ReadAccess_Str = <%=$Security_Props{'HLP_PG1_RAC_AGENTS'}%>;
  Security_WriteAccess_Str = <%=$Security_Props{'HLP_PG1_WAC_AGENTS'}%>;
  regwidth = 64;
  accesswidth = 64;
  field {
    AccessType = "RW";
    desc = "Amount of time to wait before reseting SW_TEST_AND_SET.data to 0.
            Units are specified in SW_TAS_TIMESCALE.  Accuracy is 0/+1 unit.
            Setting this to 0 disables the sw_tas_timeout feature.";
    ValRandomize = true;
  } SW_TAS_TIMEOUT[21:12] = 10'h0;
  field {
    encode = time_scale;
    AccessType = "RW";
    desc = "Units for SW_TAS_TIMEOUT test-and-set:
            * (0) 100 ns
            * (1) 1 us
            * (2) 10 us
            * (3) 100 us
            * (4) 1 ms
            * (5) 10 ms
            * (6) 100 ms
            * (7) 1 s";
    ValRandomize = true;
  } SW_TAS_TIMESCALE[11:9] = 3'h7;
  field {
    AccessType = "RW";
    desc = "Enable the timers. software should not enable timers if TICK_BASE_VAL has not be setup";
    ValRandomize = true;
  } TIMER_ENABLE[8:8] = 1'b0;
  field {
    AccessType = "RW";
    desc = "A set of timers is provided to generate ticks from 100ns to 1s. The base timer should be configured to tick every 100ns by setting TICK_BASE_VAL appropriately. The other timers  (1us,10us,100us,1ms,10ms,100ms,1s) each create a time event spaced by 10 or 2 periods of the next fastest timer when TICK_SPEEDUP is 0 or 1 respectively.";
    ValRandomize = true;
  } TICK_SPEEDUP[7:7] = 1'h0;
  field {
    AccessType = "RW";
    desc = "A zero based number that specifies the number of ports_clk cycles in 100 ns, that is 79 for a 800MHz clock";
    ValRandomize = true;
  } TICK_BASE_VAL[6:0] = 7'h4f;
};



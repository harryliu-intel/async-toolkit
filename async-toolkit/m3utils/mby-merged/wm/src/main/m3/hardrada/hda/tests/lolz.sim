( define ( bcamlookup searchkey table )
	( let loop ( ( index 0 ) ( currenttable table ) )
	( if ( = searchkey ( car currenttable ) )
	index
	( loop ( + index 1 ) ( cdr currenttable ) ) )
	)
)


<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE config PUBLIC "-//Fulcrum Microsystems//DTD DefaultCommands/EN" "DefaultCommands.dtd">
<!--
    vim:autoindent:et:sw=4:ts=4:tw=79:
-->

<config>

    <command>
        <name>set</name>
            <command>
                <name>switch</name>
                <command>
                    <name>config</name>
                    <command>
                        <filter type="family">
                            <allow>FM4000</allow>
                        </filter>
                        <name>cn</name>
                        <help>Set congestion notification properties</help>
                        <handler>tpHandleSetCN</handler>
                        <argument>
                            <name>mode</name>
                            <help>Sets the congestion notification mode</help>
                            <argument>
                                <name>off</name>
                                <help>Disable congestion notification</help>
                                <argument>
                                    <name>!&lt;cr&gt;</name>
                                    <help>Set the congestion notification mode</help>
                                </argument>
                            </argument>
                            <argument>
                                <name>vcn</name>
                                <help>Enable VOQ Congestion Notification</help>
                                <argument>
                                    <name>!&lt;cr&gt;</name>
                                    <help>Set the congestion notification mode</help>
                                </argument>
                            </argument>
                            <argument>
                                <name>bcn</name>
                                <help>Enable Backwards Congestion Notification</help>
                                <argument>
                                    <name>!&lt;cr&gt;</name>
                                    <help>Set the congestion notification mode</help>
                                </argument>
                            </argument>
                            <argument>
                                <name>scn</name>
                                <help>Enable Simple Congestion Notification</help>
                                <argument>
                                    <name>!&lt;cr&gt;</name>
                                    <help>Set the congestion notification mode</help>
                                </argument>
                            </argument>
                        </argument><!--mode-->
                    </command><!--cn-->
                </command><!--config-->
            </command><!--switch-->

    </command><!--set-->

    <command>
        <name>show</name>
            <command>
                <name>switch</name>
                <command>
                    <name>config</name>
                    <command>
                        <filter type="family">
                            <allow>FM4000</allow>
                        </filter>
                        <name>cn</name>
                        <help>Show congestion notification properties</help>
                        <handler>tpHandleShowCN</handler>
                        <argument>
                            <name>mode</name>
                            <help>show CN mode</help>
                            <argument>
                                <name>!&lt;cr&gt;</name>
                                <help>Show congestion notification mode</help>
                            </argument>
                        </argument><!--mode-->
                    </command><!--cn-->
                </command><!--config -->
            </command><!--switch -->

    </command><!--show-->

</config>

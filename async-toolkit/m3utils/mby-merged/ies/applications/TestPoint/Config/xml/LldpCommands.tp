 6,  "port-desc",        "t",    "m",    "",                               "Set port description"
 7,  "port-description", "v",    "m",    "",                               "Set port description (enter description)"

 6,  "tx-enable",        "t",    "m",    "",                               "Set port tx-enable"

 7,  "port-desc",        "t",    "m",    "",                               "Set port-description tx-enable"

#-*- shell-script -*-              For EMACS -- lets formatting happen

setenv REPO_ROOT $MODEL_ROOT

setenv NATURAL_DOCS $REPO_ROOT/verif/lib/doc/natural_docs



echo "MBY"

$NATURAL_DOCS/NaturalDocs --input $REPO_ROOT/verif/mby/ --exclude-input $REPO_ROOT/verif/mby/doc --project $REPO_ROOT/verif/mby/doc --output HTML $REPO_ROOT/verif/mby/doc/html --style Big -r

echo "LIB"

$NATURAL_DOCS/NaturalDocs --input $REPO_ROOT/verif/lib/ --exclude-input $REPO_ROOT/verif/lib/doc --project $REPO_ROOT/verif/lib/doc --output HTML $REPO_ROOT/verif/lib/doc/html --style Big -r




 

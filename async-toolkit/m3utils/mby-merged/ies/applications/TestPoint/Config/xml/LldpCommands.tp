 8,  "on",               "t",    "m",    "",                               "Set port system-description tx-enable to ON"

 7,  "sys-cap",          "t",    "m",    "",                               "Set port system-capability tx-enable"
 8,  "off",              "t",    "m",    "",                               "Set port system-capability tx-enable to OFF"
 8,  "on",               "t",    "m",    "",                               "Set port system-capability tx-enable to ON"

 7,  "all",              "t",    "m",    "",                               "Set port global tx-enable"

% Where to find the C libraries:
% link using -L/usr/local/postgresql-6.0/lib -lpq
%

if equal(TARGET, "AMD64_LINUX")
	import_sys_lib("POSTGRES95")
else
	import_lib ("pq","/usr/local/lib")
end

import ("libm3")

%
Interface ("PQ")
Interface ("PQtypes")
Interface ("PQcomm")
Interface ("PostgreSQL")
Interface ("PGRES")

%
Library ("postgresql")

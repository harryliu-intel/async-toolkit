 3,  "config",           "c",    "m",    "",                               "Sets a global switch configuration property"
 4,  "lldp",             "c",    "m",    "",                               "Control LLDP protocol"

 5,  "stop",             "c",    "m",    "tpHandleStopLldp",               "Stop LLDP protocol"

 5,  "start",            "c",    "m",    "tpHandleStartLldp",              "Start LLDP protocol"
 6,  "mac address",      "v",    "m",    "",                               "Start LLDP protocol (enter chassis MAC address)"


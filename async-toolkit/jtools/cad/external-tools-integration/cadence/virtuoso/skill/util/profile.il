; Profiling macro, usage: (Profile "file.prof" (Function ...))
(defmacro Profile (file @key (profileField ''time)
                        @rest body)
  `(let (ret)
     (profileReset)
     (profile ,profileField)
     ret=(errset ,@body)
     (profileSummary ?children t ?file ,file)
     (car ret)
     )
  )
